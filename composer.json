{
  "name": "pluginever/wp-content-pilot",
  "description": "The Best WordPress Plugin ever made!",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "homepage": "https://www.pluginever.com",
  "type": "wordpress-plugin",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "pluginever",
      "email": "support@pluginever.com"
    }
  ],
  "config": {
    "platform": {
      "php": "5.6.1"
    }
  },
  "require": {
    "php": ">=5.6.0",
    "php-curl-class/php-curl-class": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
    "wp-coding-standards/wpcs": "^2.1",
    "phpcompatibility/php-compatibility": "^9.2"
  },
  "extra": {
    "cleaner-ignore": {
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ],
    "phpcs": [
      "phpcs -s -p"
    ],
    "phpcs-pre-commit": [
      "phpcs -s -p -n"
    ],
    "phpcbf": [
      "phpcbf -p"
    ]
  }
}
